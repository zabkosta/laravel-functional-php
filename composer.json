    "require": {
         "php": "~7",
        "lstrojny/functional-php": "~1.6"
    },
    "autoload": {
        "psr-4": {
            "fwrapper\\Functional\\": "src/"
        }
    },
    "extra": {
        "laravel": {
		
		    "providers": [
                "fwrapper\\Functional\\FunctionalProvider"
